[
{% for be in __effective_backends %}
  {
    "name": {{ ('up' ~ loop.index) | tojson }},
    "provider": {{ be.provider | tojson }},
    "provider_url": {{ be.provider_url | tojson }},
    "account": {{ be.account | tojson }},
    "xtream_user": {{ be.xtream_user | tojson }},
    "xtream_pass": {{ be.xtream_pass | tojson }},
    "proxy_user": {{ be.proxy_user | tojson }},
    "proxy_pass": {{ be.proxy_pass | tojson }},
    "active_cons": {{ be.active_cons | default(None) | tojson }},
    "max_connections": {{ be.max_connections | default(None) | tojson }},
    "port": {{ be.port | int }}
  }{% if not loop.last %},{% endif %}
{% endfor %}
]
